{
    "manifest_version": 2,
    "name": "TweetDeck Improver",
    "version": "18.02.11",
    "icons": {},
    "permissions": [
    ],
    "background": {
        "scripts": []
    },
    "content_scripts": [
        {
            "matches": ["https://tweetdeck.twitter.com/*"],
            "js": [
                "shortcuts.js",
                "keepHashtags.js"
            ],
            "run_at": "document_end"
        }
    ]
}
